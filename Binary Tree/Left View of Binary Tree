//make same indexing for node right and left child then take only first time occure index.
//
class Tree
{
    void fastleftView(Node root,int key,ArrayList<Integer> list)
    {
        if(root==null) return;
        if(key==list.size())
        {
            list.add(root.data);
        }
        fastleftView(root.left,key+1,list);
        fastleftView(root.right,key+1,list);
        
    }
    //Function to return list containing elements of left view of binary tree.
    ArrayList<Integer> leftView(Node root)
    {
      // Your code here
      
      ArrayList<Integer> list=new ArrayList<>();
      fastleftView(root,0,list);
      return list;
    }
}